<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ConfirmAppointmentController">
    <children>
        <TableView fx:id="table" layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="prDate" prefWidth="97.0" text="Date" />
                <TableColumn fx:id="prHour" prefWidth="102.0" text="Hour" />
            </columns>
        </TableView>
        <Button fx:id="confirm" layoutX="271.0" layoutY="321.0" mnemonicParsing="false" text="Confirm" textFill="#ff2323">
            <effect>
                <Glow />
            </effect></Button>
        <Button layoutX="489.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Return" textFill="#ff2323">
            <effect>
                <Glow />
            </effect></Button>
        <Text layoutX="14.0" layoutY="338.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Appointments" />
        <TextField fx:id="total" layoutX="126.0" layoutY="321.0" prefHeight="1.0" prefWidth="74.0" />
    </children>
</AnchorPane>
