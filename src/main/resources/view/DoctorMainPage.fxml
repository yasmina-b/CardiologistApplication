<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DoctorMainPageController">
    <children>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="200.0" layoutY="224.0" pickOnBounds="true" preserveRatio="true" />
        <Button layoutX="212.0" layoutY="310.0" mnemonicParsing="false" onAction="#goToScheduledAppointments" text="View Scheduled Appointments" textFill="#ff2323">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
            <effect>
                <Glow />
            </effect>
        </Button>
        <Button layoutX="515.0" layoutY="27.0" mnemonicParsing="false" onAction="#goBack" text="Return" textFill="#ff2323">
            <effect>
                <Glow />
            </effect>
        </Button>
      <ImageView fitHeight="256.0" fitWidth="349.0" layoutX="136.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@appointments.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
